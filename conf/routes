# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~
# Home page

# controllers.Application.index()
GET  /getCaso          					 controllers.Application.getCaso()
POST /visitarLugar                       controllers.Application.visitarLugar()
GET  /emitirOrdenPara/:nombreVillano     controllers.Application.emitirOrdenPara(nombreVillano : String)
POST /viajar                             controllers.Application.viajar()
# Map static resources from the /public folder to the /assets URL path
GET     /*file               	controllers.Assets.at(path="/public", file)
